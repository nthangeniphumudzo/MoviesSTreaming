          },
          "Description": {
            "type": "string",
            "nullable": true
          },
          "Comment": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Comment"
            },
            "nullable": true
          },
          "Like": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Like"
            },
            "nullable": true
          },
